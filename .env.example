# Alpha Vantage API Key (Required for backup data)
# Supports multiple keys (comma-separated) for rotation
ALPHAVANTAGE_API_KEY=your_av_key_1,your_av_key_2

# Financial Modeling Prep API Key (Required for WACC & Analyst Targets)
# Supports multiple keys for high-volume analysis
FMP_API_KEY=your_fmp_key_1,your_fmp_key_2

# Google Gemini API Key (Required for AI Commentary)
GEMINI_API_KEY=your_gemini_key_1,your_gemini_key_2

# FRED API Key (Required for macro economic data - GS10, GS2, CPI)
# Get free key at: https://fred.stlouisfed.org/
FRED_API_KEY=your_fred_key_1,your_fred_key_2

# Finnhub API key
FINNHUB_API_KEY=your_finnhub_key_1,your_finnhub_key_2